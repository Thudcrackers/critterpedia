%p#notice= notice
%h1 Months
%table
  %thead
    %tr
      %th Name
      %th{:colspan => "3"}
  %tbody
    - @months.each do |month|
      %tr
        %td= month.name
        %td= link_to 'Show', month
        %td= link_to 'Edit', edit_month_path(month)
        %td= link_to 'Destroy', month, method: :delete, data: { confirm: 'Are you sure?' }
%br/
= link_to 'New Month', new_month_path
